<%
  derivatives = JSON.parse file_set.solr_document['derivatives_metadata_ssi']
  s3_loc = derivatives.first["file_location_uri"]
  http_loc = s3_loc.sub("s3://", "http://s3.amazonaws.com/")
%>

<div>
  <video controls="controls" class="video-js vjs-default-skin" style="width:100%" data-setup="{}" controlsList="nodownload" preload="auto">
    <source src="<%= http_loc %>" type="video/mp4" />
    Your browser does not support the video tag.
  </video>
</div>
